{
    "process_type": "preprocess",
    "project_type": "text",
    "languages": ["*"],
    "force_ocr": false,
    "extract_tables": false,
    "origins": {"ocr":  "aws-ocr"},
    "dataset_conf": {
        "dataset_path": "",
        "dataset_csv_path": "",
        "path_col": "Url",
        "label_col": "CategoryId"
    },
    "preprocess_conf": {
        "num_pag_ini": 0,
        "page_limit": 1000,
        "page_limit_preimage": 3,
        "parameters_preimage": [],
        "parameters_pretext": {
            "test_size": 0.2,
            "stratify": true,
            "lowercase": true,
            "remove_numbers": true,
            "simbols": true,
            "word_length": true,
            "escape_sequences": true
        }
    }
}